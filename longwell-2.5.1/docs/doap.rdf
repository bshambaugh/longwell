			<foaf:mbox rdf:resource="mailto:ryanlee@w3.org" />
		</foaf:Person>
	</maintainer>
	<maintainer>
		<foaf:Person>
			<foaf:name xml:lang="it" rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stefano Mazzocchi</foaf:name>
			<foaf:homepage rdf:resource="http://betaversion.org/~stefano/" />
		</foaf:Person>
